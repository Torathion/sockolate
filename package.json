{
  "name": "sockolate",
  "version": "1.0.1",
  "description": "Sweet, Tasty WebSockets with enriched feature flavors",
  "types": "./index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "node esbuild.config.js",
    "lint": "eslint src --fix",
    "format": "npx prettier --write */**.ts",
    "test": "vitest"
  },
  "keywords": [
    "websocket",
    "socket",
    "plugins",
    "framework",
    "utility",
    "events"
  ],
  "author": "Torathion",
  "homepage": "https://github.com/Torathion/sockolate",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Torathion/sockolate.git"
  },
  "bugs": {
    "url": "https://github.com/Torathion/sockolate/issues"
  },
  "license": "MIT",
  "type": "module",
  "sideEffects": false,
  "packageManager": "pnpm@10.11.0",
  "devDependencies": {
    "@rollup/rollup-win32-ia32-msvc": "^4.41.1",
    "@size-limit/file": "^11.2.0",
    "@size-limit/preset-app": "^11.2.0",
    "@vitest/coverage-v8": "^3.2.1",
    "esbuild": "^0.25.5",
    "eslint": "^9.28.0",
    "eslint-config-shiny": "^4.2.1",
    "jiti": "^2.4.2",
    "mock-socket": "^9.3.1",
    "size-limit": "^11.2.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.1"
  },
  "dependencies": {
    "compresso": "^1.4.1",
    "mitt": "^3.0.1",
    "typestar": "^1.12.0"
  }
